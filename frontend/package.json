{
  "name": "chandrahoro-v2",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" \"*.{json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" \"*.{json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lighthouse": "lhci autorun",
    "lighthouse:desktop": "lighthouse http://localhost:3000 --preset=desktop --output=html --output-path=./lighthouse-reports/desktop-report.html",
    "lighthouse:mobile": "lighthouse http://localhost:3000 --preset=mobile --output=html --output-path=./lighthouse-reports/mobile-report.html",
    "perf:audit": "npm run build && npm run start & sleep 10 && npm run lighthouse && kill %1",
    "perf:analyze": "npm run build && npx next-bundle-analyzer",
    "analyze": "ANALYZE=true npm run build",
    "bundle:size": "npm run build && npx bundlesize",
    "bundle:report": "npm run build && npx webpack-bundle-analyzer .next/static/chunks/",
    "a11y": "pa11y http://localhost:3000",
    "a11y:ci": "pa11y-ci --sitemap http://localhost:3000/sitemap.xml",
    "test:final-polish": "npx tsx scripts/test-final-polish.ts",
    "test:mobile": "npx tsx scripts/test-mobile-responsive.ts",
    "test:dashboard": "npx tsx scripts/test-dashboard.ts",
    "check:types": "tsc --noEmit",
    "check:all": "npm run type-check && npm run lint && npm run format:check",
    "prepare": "husky install",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "postinstall": "prisma generate"
  },
