    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.8.2",
    "lighthouse-ci": "^1.13.1",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
